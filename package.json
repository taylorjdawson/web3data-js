{
  "name": "web3data-js",
  "version": "0.3.11",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build": "rm dist/web3data.min.js; parcel build index.js --target browser --out-dir dist --out-file web3data.min.js",
    "lint": "xo",
    "lintfix": "xo --fix",
    "test": "xo --quiet && cross-env  nyc -s ava",
    "test-record": "cross-env  nyc -s ava",
    "test-live": "nyc -s ava",
    "test-fast": "cross-env nyc -s ava --fail-fast",
    "test-verbose": "cross-env nyc ava --verbose",
    "clean-recs": "rm -r test/recordings",
    "example": "superstatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "blockchain",
    "web3",
    "data"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@pollyjs/core": "^2.6.0",
    "@pollyjs/persister-fs": "^2.6.0",
    "@pollyjs/adapter-node-http": "^2.6.0",
    "ava": "^2.2.0",
    "capture-console": "^1.0.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "get-port": "^5.0.0",
    "nyc": "^14.1.1",
    "parcel": "^1.12.3",
    "prettier": "^1.18.2",
    "superstatic": "^6.0.4",
    "xo": "^0.24.0",
    "lodash": "^4.17.14"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "isomorphic-ws": "^4.0.1",
    "ws": "^7.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "prettier": true
  }
}
